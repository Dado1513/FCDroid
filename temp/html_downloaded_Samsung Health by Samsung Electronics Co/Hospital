	<!-- viewport//end -->
	<script src="/js/jquery-1.11.2.min.js" type="text/javascript"></script>
	<script type="text/javascript">
	/* 위로
	스크롤이 특정 위치로 이동하면 위로버튼이 나타난다.
	위로버튼을 클릭하면 상단으로 이동 */
	function btn_mv_up(oj) {
		if(!oj) return false;
		var o = $(oj);
		var p = $(window).scrollTop();
		if(p > 300){ o.fadeIn('slow'); } // 위로버튼이 나타나는 위치 지정
		else if(p < 300){ o.fadeOut('slow'); } // 위로버튼을 숨기는 위치 지정
	}

	// 위로 버튼
	$(document).scroll(function() {
		btn_mv_up('.bottomFixed');
		}).on('click', '.goTop', function() {
		//$("html, body").animate({scrollTop:0}, 'slow');		// 부드럽게 상단으로 올라가는 animation
	});
	
	<!--
		$(document).ready(function(){

		});
	//-->
	</script>
</body>
</html>