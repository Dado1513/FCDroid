    },

    "pt": {
      "title": "humano ou robô?",
      "subTitle": "Assinale a opção abaixo provando que você é humano",
    },

    "es": {
      "title": "¿Eres humano o robot?",
      "subTitle": "Haz clic en la casilla de abajo para probar que eres humano",
    },
    "en": {
      "title": "human or robot?",
      "subTitle": "Prove you are a human by clicking the check box below",
    },
    "fr": {
      "title": "humain ou robot?",
      "subTitle": "prouvez que vous êtes humain en cochant la case ci-dessous",
    },
  }
  var lang = getParameterByName("lang").toLowerCase().substr(0,2);

  var langStr = i18n[lang] || i18n["en"];

  $("#title").html(langStr["title"]);
  $("#subTitle").html(langStr["subTitle"]);


  var script = document.createElement('script');
  script.type = 'text/javascript';
  script.src = "https://www.google.com/recaptcha/api.js?onload=onloadCallback&render=explicit&hl="+lang;
  document.body.appendChild(script);
  script.addEventListener('error', errload, false);

  var verifyCallback = function(response) {
    verifykey = response;
    if (window.webkit && window.webkit.messageHandlers && window.webkit.messageHandlers.app) {
      window.webkit.messageHandlers.app.postMessage(verifykey);
    }
    if (window.verify && window.verify.success) {
      verify.success(verifykey);
    }
  };
  var widgetId;
  var verifykey;
  var sitekey = "6LcdkCgTAAAAANKysvspt6pQWgYj_1FZhzoNyHLW"
  var onloadCallback = function() {
    $("#grecaptcha").empty();
    widgetId = grecaptcha.render('grecaptcha', {
      'sitekey': sitekey,
      'callback': verifyCallback,
    });
  }

  /**
   * Determine the mobile operating system.
   * This function returns one of 'iOS', 'Android', 'Windows Phone', or 'unknown'.
   *
   * @returns {String}
   */
  function getMobileOperatingSystem() {
    var userAgent = navigator.userAgent || navigator.vendor || window.opera;

    // Windows Phone must come first because its UA also contains "Android"
    if (/windows phone/i.test(userAgent)) {
      return "Windows Phone";
    }

    if (/android/i.test(userAgent)) {
      return "Android";
    }

    // iOS detection from: http://stackoverflow.com/a/9039885/177710
    if (/iPad|iPhone|iPod/.test(userAgent) && !window.MSStream) {
      return "iOS";
    }

    return "unknown";
  }


  $(function() {
    var mobile = getMobileOperatingSystem();
    // alert(mobile);
    $("#continue").on("click", function(e) {
      e.preventDefault();
      if (!verifykey) {
        $(".message").addClass("visible");
      } else {
        $(".message").removeClass("visible");
        if (window.webkit && window.webkit.messageHandlers && window.webkit.messageHandlers.app) {
          window.webkit.messageHandlers.app.postMessage(verifykey);
        }
        if (window.verify && window.verify.success) {
          verify.success(verifykey);
        }
      };
    })
    setInterval(function(){
      var grecaptchaDiv = $(".container").siblings("div")
      if (grecaptchaDiv.length > 0) {
        if (grecaptchaDiv.css("visibility") == 'hidden') {
          grecaptchaDiv.css("top","-10000px")
        } else {
          grecaptchaDiv.css("top","10px")
        }
      }

    })
  });
  </script>
