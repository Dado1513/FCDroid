<script>
  var clubWls = window.localStorage;
  var clubVersionKey = 'clubVersion';
  var clubStartDiyKey = 'clubStartDiy';
  var clubMessageReferKey = 'clubMessageRefer';
  var clubCurrentVersion = '12';
  var clubStartDiyReferKey = 'clubStartRefer';
  var clubFromReferKey = 'clubFromRefer';
  var clubListIndexKey = 'clubListIndex';
  var clubIndexPositonInfoKey = 'clubIndexPositonInfo';
  var clubThemePositonInfoKey = 'clubThemePositonInfo';
  var clubPagePositionKey = 'clubPagePosition';

  (function(){
  	$('.cmclub_mes').on('touchstart',function(){//记录消息箱入口来源
  		clubWls.setItem(clubMessageReferKey,location.href);
  	});
  })();

</script>

<script>

   

  if(typeof Activity != 'undefined'){//是客户端 注入startdiy按钮
    document.write('<div class="cmclub_silder"><div class="cmclub_diy_explain_tip"><div class="cmclub_diy_explain_arrow"></div><div class="cmclub_diy_explain_box"><p class="cmclub_diy_explain_text">Tap here to DIY your theme</p><a class="cmclub_diy_explain_ok">OK</a></div></div></div>');
  }


  if(clubWls.getItem(clubStartDiyKey) == null){
      $('.cmclub_diy_explain_tip').show();
      clubWls.setItem(clubStartDiyKey,true);
  }

   $(document).on('click','.cmclub_silder',function(event){//点击startdiy按钮
   		//event.stopPropagation();
  		if(typeof DIY != 'undefined'){
      $('.cmclub_diy_explain_tip').hide();
      //clubWls.setItem(clubStartDiyKey,true);
      clubWls.setItem(clubStartDiyReferKey,'theme');
      DIY.startDiy();
    }else{
      return false;
    }
    });

  $(document).on('click', '.cmclub_diy_explain_ok', function(event) {//点击diy引导确认按钮
    event.preventDefault();
    event.stopPropagation();
    $('.cmclub_diy_explain_tip').hide();
    //clubWls.setItem(clubStartDiyKey,true);
  });

</script>


<script>

var o_cmclub_eggs = document.getElementById('cmclub_eggs');


  var debugClickCount = 0;
  var debugClickLastTime = -1;

  o_cmclub_eggs.addEventListener('touchstart',function(e){   

      var now = e.timeStamp || Date.now();
      if(now - debugClickLastTime > 2000){//假设没有连续的4次 而是经过很长时间 那么这一次被认为是第一次
          debugClickCount = 1;
          debugClickLastTime = now;
      }else{
          ++debugClickCount;
          if(debugClickCount === 5){
               debugClickCount = 0;
               debugClickLastTime = -1;
               location.href = 'http://theme.cmcm.com?f=caidan';
          }
      }
  })

</script>




