  }

  if(clubWls.getItem(clubVersionKey) == null){//清除缓存
    clearClubCache();
    clubWls.setItem(clubVersionKey,clubCurrentVersion);
  }


  if(clubWls.getItem(clubVersionKey) != clubCurrentVersion){//清除缓存
    clearClubCache();
    clubWls.setItem(clubVersionKey,clubCurrentVersion);
  }


