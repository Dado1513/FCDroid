            cmclubReport(false,"launcher_club_dig",{"fid":41,"position":maxThemeNum,"page":1});
            clubWls.removeItem(clubThemePositonInfoKey);
          }
         
        Activity.finishPresentActivity();
      }else{
        return false;
      }
  }


