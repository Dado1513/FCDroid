  function cmclubReport(bol,page,obj){
    if(typeof WebView != 'undefined'){
      var dd = JSON.stringify(obj).replace(/\{/g,'[').replace(/\}/g,']').replace(/\:/g,',')
      WebView.report(bol,page,dd);
    }
  }


  //ss.innerHTML = 102;
  function clientOnBackPressed(){
    //ss.innerHTML += 'clubThemePositonInfoKey:'+clubWls.getItem(clubThemePositonInfoKey)+'<br>';
      if(typeof Activity != 'undefined'){
                  if(clubWls.getItem(clubThemePositonInfoKey)){
