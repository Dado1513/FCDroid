    jQuery('#search-help').add('#search-help-bottom').submit(
        function () {
            var input_value = this.help_keywords.value;
            var s = s_gi('acsus-prod');
            s.linkTrackVars = 'events,eVar47,prop46';
            s.linkTrackEvents = 'event1,event2,event3,event4,event5,event6,event7,event27';
            s.events = "event27";
            s.eVar47 = input_value;
            s.prop46 = input_value;

            s.tl(this,'o', input_value);

        }
    );

    P.when('wcs-help-faceted-update', 'ready').execute('help-facet-omniture', function() {
        jQuery('.cs-help-search-facets li a').click(
            function (e) {
                var s = s_gi('acsus-prod');
                s.linkTrackVars = 'events,eVar48,eVar49,eVar50';
                s.linkTrackEvents = 'event1,event2,event3,event4,event5,event6,event7,event28';
                s.events = 'event28';

                var link = jQuery(this);
                var facetName = link.closest('fieldset').find('legend').text();
                var facetValue = link.text();
                s.eVar48 = '' + '|' + facetName + '|' + facetValue;
                s.eVar49 = facetName;
                s.eVar50 = facetValue;

                s.tl(this,'o', link.text());
            }
        );
    });

    var answerBoxLinks = jQuery(".answer-box a").filter(function() {
                                return jQuery(this).attr('href');
                            });
    answerBoxLinks.click(
        function(e) {
            var s = s_gi('acsus-prod');
            s.linkTrackVars = 'events,eVar9,eVar10,prop10,list1';
            s.linkTrackEvents = 'event1,event2,event3,event4,event5,event6,event7,event13';
            s.events = 'event13';
            s.prop10 = '';
            s.eVar10 = '';
            s.list1 = 'CS_HELP_GATEWAY_SS_V2_57525:T3';

            var link = jQuery(this);
            s.tl(this, 'o', link.text());
        }
    );




   /************* DO NOT ALTER ANYTHING BELOW THIS LINE ! **************/
   var s_code=s.t();
       if(s_code) { 
           jQuery("body").append(s_code);
       }

   //-->

};
