		var cartLookupId = '' || '';
		var ownMobileNumber = '' || '';
		//var cartRechargeData = JSON.parse(('' || '{}').replace(/&quot;/g,'"'));
		document.addEventListener('DOMContentLoaded',function(event){
			function getCartProductName(productType){
				var map = {
					'V' : 'Prepaid',
					'M' : 'Postpaid',
					'D' : 'DTH',
					'C' : 'datacard',
					'F' : 'PostpaidDataCard',
					'Z' : 'Metro',
					'E' : 'Electricity',
					'G' : 'Gas',
					'W' : 'Water',
					'Y' : 'Broadband',
					'L' : 'Landline'
				}
				return map[productType] || 'Prepaid';
			}
			if(cartLookupId){
				atomic.get('/mobile/cart/'+cartLookupId).success(function(result){
					fc.fireClevertapEvent({
						name : 'Abandoned cart',
						data : {
							'Amount' : (result && result.rechargeData && result.rechargeData.rechargeAmount) || '',
							'Product Type' : getCartProductName((result && result.rechargeData && result.rechargeData.productName) || ''),
