							'Number recharged': (result && result.rechargeData && result.rechargeData.serviceNumber) || '',
							'Operator': (result && result.rechargeData && result.rechargeData.operatorName) || '',
						}
					})
				})
			}

            (function handleWebViewHeader() {
                var websiteHeader = document.querySelector('header.header') || document.querySelector('div.transactions-header');
                var webViewHeader = document.querySelector('div.webview-header');
                if(websiteHeader && webViewHeader) {
                    webViewHeader.style.display = 'none';
