<script language="JavaScript" type="text/javascript">
document.addEventListener('DOMContentLoaded',function(event){
  if(typeof fc === 'undefined'){
    fc = {};
  }
  fc.fireClevertapEvent = function(eventObj){
    if(clevertap){
      clevertap.event.push(eventObj.name,eventObj.data);
    }
  }
  fc.setProfile = function(profileObj){
    if(clevertap){
      clevertap.profile.push(profileObj)
    }
  }
  fc.onUserLogin = function(profileObj){
    if(clevertap && clevertap.onUserLogin){
      clevertap.onUserLogin.push(profileObj)
    }
  }
  fc.logoutUser = function(){
    if(clevertap && clevertap.logout){
      clevertap.logout();
    }
  }
})
</script>
